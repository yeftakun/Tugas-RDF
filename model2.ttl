@prefix tm2: <http://tugasmodel2.org/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .

######## Mahasiswa ########
tm2:mahasiswa1 a tm2:Mahasiswa ;
    tm2:nama "Yefta" ;
    tm2:sukaMinum tm2:Americano ;
    tm2:seringBeliDi tm2:JanjiJiwa .

tm2:mahasiswa2 a tm2:Mahasiswa ;
    tm2:nama "Sinta" ;
    tm2:sukaMinum tm2:Latte ;
    tm2:seringBeliDi tm2:KopiKenangan .

tm2:mahasiswa3 a tm2:Mahasiswa ;
    tm2:nama "Budi" ;
    tm2:sukaMinum tm2:KopiTubruk ;
    tm2:seringBeliDi tm2:WarungKopi .

######## Jenis Kopi ########
tm2:Americano a tm2:Kopi ;
    tm2:jenis "Hitam" .

tm2:Latte a tm2:Kopi ;
    tm2:jenis "Susu" .

tm2:KopiTubruk a tm2:Kopi ;
    tm2:jenis "Tradisional" .

######## Kedai Kopi ########
tm2:JanjiJiwa a tm2:KedaiKopi ;
    tm2:lokasi "Kampus A" .

tm2:KopiKenangan a tm2:KedaiKopi ;
    tm2:lokasi "Mall B" .

tm2:WarungKopi a tm2:KedaiKopi ;
    tm2:lokasi "Pasar C" .
